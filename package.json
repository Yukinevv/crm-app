{
  "name": "crm-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "json-server": "json-server --watch functions/db.json --port 3000",
    "emulators": "firebase emulators:start --only functions,ui",
    "start-front": "wait-on http://localhost:9099 && ng serve --proxy-config proxy.conf.json",
    "dev": "concurrently \"npm run json-server\" \"npm start\"",
    "dev-em": "concurrently \"npm run emulators\" \"npm run json-server\" \"npm run start\""
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^19.2.14",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/fire": "^19.1.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@fullcalendar/angular": "^6.1.18",
    "@fullcalendar/daygrid": "^6.1.18",
    "@fullcalendar/interaction": "^6.1.18",
    "@fullcalendar/timegrid": "^6.1.18",
    "bootstrap": "^4.6.2",
    "file-saver": "^2.0.5",
    "firebase": "^10.14.1",
    "jquery": "^3.7.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "papaparse": "^5.5.3",
    "popper.js": "^1.16.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/cli": "^19.2.15",
    "@angular/compiler-cli": "^19.2.0",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~5.1.0",
    "@types/papaparse": "^5.3.16",
    "concurrently": "^9.2.0",
    "jasmine-core": "~5.6.0",
    "json-server": "^0.16.1",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2",
    "wait-on": "^8.0.4"
  }
}
